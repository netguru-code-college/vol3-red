
  <div class="jumbotron mt-4">
    <h1 class="display-5 mb-5">Building of number <%= @building.building_number %></h1>
    <div class="container">

      <div class="row">
        <div class="col">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Details</th>
                <th scope="col">Address</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>Street</td>
                <td><%= @building.street.capitalize %></td>

              </tr>
              <tr>
                <td>Building number</td>
                <td><%= @building.building_number %></td>
              </tr>
              <tr>
                <td>Zip code:</td>
                <td><%= @building.zip_code %></td>
              </tr>
              <tr>
                <td>Estate:</td>
                <td><%= @estate.name %></td>
              </tr>
            </tbody>
          </table>
        </div>
        <div class="col">
          <div style="width: 100%"><iframe width="100%" height="400" src="https://maps.google.com/maps?width=100%&amp;height=600&amp;hl=en&amp;q=1%20Grafton%20Street%2C%20Dublin%2C%20Ireland+(My%20Business%20Name)&amp;ie=UTF8&amp;t=&amp;z=14&amp;iwloc=B&amp;output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"><a href="https://www.maps.ie/create-google-map/">Embed Google Map</a></iframe>
          </div>
        </div>
      </div>
    </div>
  </div>
