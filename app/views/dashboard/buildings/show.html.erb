
  <div class="jumbotron mt-4">
    <h1 class="display-5 mb-5">Building of number <%= @building.building_number %></h1>
    <div class="container">

      <div class="row">
        <div class="col">
          <table class="table">
            <thead>
              <tr>
                <th scope="col">Details</th>
                <th scope="col">Address</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>Street</td>
                <td><%= @building.street.capitalize %></td>

              </tr>
              <tr>
                <td>Building number</td>
                <td><%= @building.building_number %></td>
              </tr>
              <tr>
                <td>Zip code:</td>
                <td><%= @building.zip_code %></td>
              </tr>
              <tr>
                <td>Estate:</td>
                <td><%= @estate.name %></td>
              </tr>
            </tbody>
          </table>
        </div>
        <div class="col">
          <div style="width: 100%">
	    <iframe width="300" height="300" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?key=ytw5ig0mGYaEn9rf3H8ylpEOYNY=&q=ADDRESS_OR_COORDINATES"
		    allowfullscreen>
	    </iframe>
          </div>
        </div>
      </div>
    </div>
  </div>
